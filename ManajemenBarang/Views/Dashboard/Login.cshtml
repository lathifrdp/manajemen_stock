@model ManajemenBarang.Models.GetUsup2_Result
@{
    ViewBag.Title = "Login";
}
<style>
    .my-style {
        outline: none;
        border: none;
        padding-left: 75px;
    }
</style>
<title>Form Login</title>
<center>
    <h2>Login</h2>
    <br />
    <br />
    <div class="row">
        <div class="col-md-12">

            <form method="post" action="@Url.Action("LoginData", "Dashboard")">
                <div class="col-md-12">
                    
                    <table>
                          <tr>
                              <td>@Html.LabelFor(a => a.username)</td>
                              <td>@Html.TextBoxFor(a => a.username,new {required = "required", type = "email" })</td>
                          </tr>

                        <tr>
                            <td>
                                @Html.LabelFor(a => a.password)
                            </td>
                            <td>
                                @Html.PasswordFor(a => a.password, new { required = "required" })
                            </td>
                        </tr>
                        <tr>
                            <td></td>
                            <td>
                                <input type="submit" value="Login" />
                            </td>
                            <td></td>
                        </tr>
                    </table>

